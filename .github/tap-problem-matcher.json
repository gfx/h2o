{
  "problemMatcher": {
    "owner": "tap-problem-matcher",
    "pattern": [
      {
        "regexp": "^\\s*(#\\s+Failed test .*)",
        "message": 1
      },
      {
        "regexp": "^\\s*(#\\s+at (\\S+) line (\\d+)\\.)",
        "message": 1,
        "file": 2,
        "line": 3
      },
      {
        "regexp":  "^\\s*(#.*)",
        "message": 1,
        "loop": true
      }
    ]
  }
}
